	<queryString>
		<![CDATA[select
coalesce(( select name from ad_org where ad_org_id = $P{AD_Org_ID} ), ' ' ) as L_ORG,
coalesce(( select name from c_bp_group where c_bp_group_id = $P{C_BP_Group_ID} ), ' ' ) as L_GROUP,
coalesce((select name from c_bpartner where c_bpartner_id = $P{C_BPartner_ID} ), ' ' ) as L_BP,
max(bp.value) as bpvalue, max(bp.name) as name, max(bp.SO_creditlimit) creditLimit,
'Solde Depart' as doctype,
null as documentno,
null as acctno,
$P{DateFrom} as dateacct,
0 as DOCAMTDB,
0 as DOCAMTCR,
(select
	(select
		coalesce(
  		sum(Z_Invoice_Open_AsOf_Base(i.C_Invoice_ID,0,$P{DateFrom}))
		,0)
  		from c_invoice i
  		where i.c_bpartner_id = bp.c_bpartner_id
    		and i.dateacct < $P{DateFrom}
    		and ( ( i.IsSOTrx = 'Y' and $P{IsSOTrx} = 'Y' ) or ( i.IsSOTrx = 'N' and $P{IsSOTrx} = 'N' ) )
    		and i.docstatus in ('CO','CL','RE','VO')
    		and ( i.AD_Org_ID = $P{AD_Org_ID} or $P{AD_Org_ID} is null or $P{AD_Org_ID} = 0 )
	)
	-
	(select
		coalesce(
		sum(
			CurrencyConvert(
                (p.PayAmt - coalesce( ( select sum(( zpaysign(p.isreceipt) * al.Amount ))
	                               from c_allocationline al
	                               inner join c_allocationhdr ah on ah.c_allocationhdr_id = al.c_allocationhdr_id
                                   where al.c_payment_id = p.c_payment_id
								   and  ah.dateacct <= $P{DateFrom}
								   and  al.isactive = 'Y'
                                   group by al.c_payment_id ), 0 ) ),
	              p.C_Currency_ID, ( select a.C_Currency_ID from c_acctschema a where a.ad_client_id = p.ad_client_id limit 1) , p.DateAcct, p.C_ConversionType_ID, p.ad_client_id, p.ad_org_id
    		)
		)
		,0)
		from c_payment p
		where p.c_bpartner_id = bp.c_bpartner_id
  			and p.dateacct < $P{DateFrom}
  			and ( ( p.IsReceipt = 'Y' and $P{IsSOTrx} = 'Y' ) or ( p.IsReceipt = 'N' and $P{IsSOTrx} = 'N' ) )
  			and p.docstatus in ('CO','CL','RE','VO')
  			and (p.c_charge_id is null or p.c_charge_id = 0)
  			and ( p.AD_Org_ID = $P{AD_Org_ID} or $P{AD_Org_ID} is null or $P{AD_Org_ID} = 0 )
  	)

from dual ) as DOCSOLDE,
null AS Extrait,
'en_US' as AD_LANGUAGE
from c_bp_group bpg,
     c_bpartner bp
where
    ( bpg.Value >= $P{BP_GROUP_FROM} or $P{BP_GROUP_FROM} is null or $P{BP_GROUP_FROM} = '' )
and ( bpg.Value <= $P{BP_GROUP_TO} or $P{BP_GROUP_TO} is null or $P{BP_GROUP_TO} = '' )
and ( bp.C_BP_Group_ID = bpg.C_BP_Group_ID )
and ( bp.C_BPartner_ID = $P{C_BPartner_ID} or $P{C_BPartner_ID} is null or $P{C_BPartner_ID} = 0 )
and ( bp.C_BP_Group_ID = $P{C_BP_Group_ID} or $P{C_BP_Group_ID} is null or $P{C_BP_Group_ID} = 0 )
and ( (bp.iscustomer = 'Y' and $P{IsSOTrx} = 'Y') or (bp.isvendor = 'Y' and $P{IsSOTrx} = 'N'))
and ( bp.ad_client_id = $P{AD_Client_ID} or $P{AD_Client_ID} is null or $P{AD_Client_ID} = 0 )
GROUP BY bp.C_BPartner_ID
UNION
select
coalesce(( select name from ad_org where ad_org_id = $P{AD_Org_ID} ), ' ' ) as L_ORG,
coalesce((select name from c_bp_group where c_bp_group_id = $P{C_BP_Group_ID}), ' ' ) as L_GROUP,
coalesce((select name from c_bpartner where c_bpartner_id = $P{C_BPartner_ID}), ' ' ) as L_BP,
bp.value as bpvalue, bp.name as name, bp.SO_creditlimit creditLimit,
j.tablename as doctype,
j.documentno  || ' ' || j.zfact_acct_id as documentno,
ev.value as acctno,
case when (fa.ad_table_id = 318) then (select dateacct from C_Invoice where c_invoice_id=fa.record_id)
     when (fa.ad_table_id = 335) then (select dateacct from C_Payment where c_payment_id=fa.record_id)
     when (fa.ad_table_id = 407) then (select dateacct from C_cash where c_cash_id=fa.record_id)	-- c_cash
     else fa.dateacct
end as dateacct,
case when ($P{IsSOTrx} = 'Y' and fa.ad_table_id = 318) then fa.amtacctdr - fa.amtacctcr
     when ($P{IsSOTrx} = 'N' and fa.ad_table_id = 335) then -1 * (fa.amtacctdr - fa.amtacctcr)
	 when ($P{IsSOTrx} = 'N' and fa.ad_table_id = 407) then fa.amtacctdr - fa.amtacctcr	-- c_cash
     when ($P{IsSOTrx} = 'N' and fa.ad_table_id in (390,735)) then -1 * (fa.amtacctdr - fa.amtacctcr)
     else 0
end as DOCAMTDB,
case when ($P{IsSOTrx} = 'Y' and fa.ad_table_id = 335) then fa.amtacctdr - fa.amtacctcr
	 when ($P{IsSOTrx} = 'Y' and fa.ad_table_id = 407) then -1 * (fa.amtacctdr - fa.amtacctcr)			-- c_cash
     when ($P{IsSOTrx} = 'N' and fa.ad_table_id = 318) then -1 * (fa.amtacctdr - fa.amtacctcr)
     when ($P{IsSOTrx} = 'Y' and fa.ad_table_id in (390,735)) then (fa.amtacctdr - fa.amtacctcr)
     else 0
end as DOCAMTCR,
0 as DOCSOLDE,
case when (fa.ad_table_id = 335) then (select distinct bs.NAME from C_BankStatementLine  bsl
	 	  				  		 	  inner join C_BankStatement bs on bs.C_BankStatement_ID = bsl.C_BankStatement_ID
									  and bs.docstatus in ('CO','CL')
									  where bsl.C_Payment_ID = fa.record_id)
     else null
end as Extrait,
'en_US' as AD_LANGUAGE
from fact_acct fa
inner join c_bpartner bp on bp.C_BPartner_ID = fa.C_BPartner_ID
inner join c_bp_group bpg on bp.C_BP_Group_ID = bpg.C_BP_Group_ID
left outer join gl_category glc on glc.gl_category_id = fa.gl_category_id
inner join c_elementvalue ev on ev.c_elementvalue_id = fa.account_id
inner join z_journal j on j.zfact_acct_id = fa.fact_acct_id
where
    ( bpg.Value >= $P{BP_GROUP_FROM} or $P{BP_GROUP_FROM} is null or $P{BP_GROUP_FROM} = '' )
and ( bpg.Value <= $P{BP_GROUP_TO} or $P{BP_GROUP_TO} is null or $P{BP_GROUP_TO} = '' )
and ( bp.C_BPartner_ID = $P{C_BPartner_ID} or $P{C_BPartner_ID} is null or $P{C_BPartner_ID} = 0 )
and ( bp.C_BP_Group_ID = $P{C_BP_Group_ID} or $P{C_BP_Group_ID} is null or $P{C_BP_Group_ID} = 0 )
and (($P{IsSOTrx} = 'Y' and bp.iscustomer = 'Y') or ($P{IsSOTrx} = 'N' and bp.isvendor = 'Y'))
and ( fa.AD_Org_ID = $P{AD_Org_ID} or $P{AD_Org_ID} is null or $P{AD_Org_ID} = 0 )
and fa.ad_client_id = $P{AD_Client_ID}
and fa.dateacct >= $P{DateFrom}
and fa.dateacct <= $P{DateTo}
and (
	-- Facture
	(fa.ad_table_id in (318)
		and
		-- ev.value like N'4%' Pas bon car on prend les 401 et 4456 (Tva) et 499 facture de reprise)
		-- fa.c_tax_id is null -- Facile ! c est le seul fact acct qui n a pas de tax
		$P{IsSOTrx} = (select issotrx from c_invoice where c_invoice_id = fa.record_id)
		and fa.c_tax_id is null
		-- Chez simop c_tax_id est 0 sur les vieux records 251
		and fa.account_id not in (select account_id from c_validcombination where c_validcombination_id in (select t_due_acct from c_tax_acct))
		and fa.account_id not in (select account_id from c_validcombination where c_validcombination_id in (select t_liability_acct from c_tax_acct))
		and fa.account_id not in (select account_id from c_validcombination where c_validcombination_id in (select t_credit_acct from c_tax_acct))
		and fa.account_id not in (select account_id from c_validcombination where c_validcombination_id in (select t_receivables_acct from c_tax_acct))
	)
	-- Paiement
	or (fa.ad_table_id in (335)
		and
		(ev.value like '5%' -- Le Compte Banque
		or ev.value like 'X5%' -- Solideal UK
		or ev.value like '403%' -- BOR
		or ev.value like '413%' -- LCR
                or ev.value like '467%' -- Factor
		or ev.value like '4013%' -- Encaissement
		)
		and $P{IsSOTrx} = (select isreceipt from c_payment where c_payment_id = fa.record_id)
            and ((select C_Charge_ID from C_Payment where c_payment_id = fa.record_id) is null or (select C_Charge_ID from C_Payment where c_payment_id = fa.record_id) = 0 )
	)

	-- C_Cash (pour les clients qui utilisent le C_Cash pour gérer la caisse)
	or (fa.ad_table_id in (407)
		and
		(ev.value like '401%' -- fournisseurs
		or ev.value like '411%' -- clients
		)
		and $P{IsSOTrx} = (select inv.IsSOTrx from c_invoice inv where inv.c_invoice_id = (select cl.c_invoice_id from c_cashline cl where cl.c_cashline_id = fa.line_id))
	)

	--end C_Cash



	-- Affectation de Paiement
	or (fa.ad_table_id in (390,735)
		and ev.value not like '4%'
		and ev.value not like 'X4%' -- Solideal UK
	)
       and $P{IsSOTrx}  =(select distinct coalesce(i.issotrx,p.isreceipt) from c_allocationline al
	 		 left outer join c_invoice i on al.c_invoice_id=i.c_invoice_id
			 left outer join c_payment p on al.c_payment_id=p.c_payment_id
			 where al.c_allocationhdr_id = fa.record_id)
)
order by 4, 10, 7]]>
	</queryString>
