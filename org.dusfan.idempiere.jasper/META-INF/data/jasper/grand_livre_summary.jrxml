	<queryString>
		<![CDATA[Select coalesce(sum(coalesce(f.amtacctdr,0) - coalesce(f.amtacctcr,0)),0) as amount,
coalesce(sum(f.amtacctdr),0) as amountdr,
coalesce(sum(f.amtacctcr),0) as amountcr,
concat(acct.value,' ') as acct_value,
acct.name as acct_name,
Z_Id_Cpt_To_Val($P{Account_From}) as iFromAcc,
Z_Id_Cpt_To_Val($P{Account_To}) as iToAcc,
coalesce(z_account_starting_balance(acct.ad_client_id, $P{AD_Org_ID}, ci.c_acctschema1_id, acct.c_elementvalue_id, $P{DateFrom}), 0) as starting_balance,
ac.name as client_name,
org.name as org_name,
bse.iso_code as base_curr,
'en_US'  as AD_LANGUAGE
from c_elementvalue acct
inner join c_element e on e.c_element_id = acct.c_element_id and e.elementtype = 'A'
left outer join fact_acct f on acct.c_elementvalue_id = f.account_id and f.postingtype = 'A'
  	and f.dateacct >= $P{DateFrom}
	and f.dateacct <= $P{DateTo}
  	and (f.ad_Org_id = $P{AD_Org_ID} or $P{AD_Org_ID} = 0 or f.ad_Org_ID = 0)
  	and (f.ad_Client_id = $P{AD_Client_ID} or $P{AD_Client_ID} = 0 or f.ad_Client_ID = 0)
  	and (f.C_Activity_ID = $P{C_Activity_ID} or $P{C_Activity_ID} is null or $P{C_Activity_ID} = 0)
left outer join c_currency c on c.c_currency_id = f.c_currency_id
inner join ad_client ac on ac.ad_client_id = $P{AD_Client_ID}
inner join ad_Org org on org.ad_org_id = $P{AD_Org_ID}
inner join ad_clientinfo ci on ci.ad_client_id = $P{AD_Client_ID}
inner join c_acctschema cas on cas.c_acctschema_id = ci.c_acctschema1_id
inner join c_currency bse on bse.c_currency_id = cas.c_currency_id
left outer join C_PERIOD per on per.C_PERIOD_ID = f.C_PERIOD_ID
where
acct.issummary = 'N'
   and ( f.ad_client_id = $P{AD_Client_ID} )
  and ( f.ad_org_id = $P{AD_Org_ID} or  $P{AD_Org_ID} = 0 )
  and (acct.ad_client_id = 0 or acct.ad_client_id = $P{AD_Client_ID} or $P{AD_Client_ID} = 0 )
  and ( Z_Id_Cpt_To_Val(acct.c_elementvalue_ID) >= Z_Id_Cpt_To_Val($P{Account_From}) or $P{Account_From} = 0)
  and ( Z_Id_Cpt_To_Val(acct.c_elementvalue_ID) <= Z_Id_Cpt_To_Val($P{Account_To}) or $P{Account_To} = 0)
  and ( per.PERIODTYPE is null or per.PERIODTYPE='S' or 'Y'='Y')
  and (f.C_Activity_ID = $P{C_Activity_ID} or $P{C_Activity_ID} is null or $P{C_Activity_ID} = 0)
group by acct.ad_client_id, acct.ad_org_id, ci.c_acctschema1_id, acct.c_elementvalue_id,acct.value ,acct.name,ac.name, bse.iso_code, org.name
order by acct_value]]>
