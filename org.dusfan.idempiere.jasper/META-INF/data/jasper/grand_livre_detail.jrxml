	<queryString>
		<![CDATA[select
0 as amount,
0 as amountCR,
0 as amountDR,
0 as source_amount,
$P{DateFrom} as dateacct,
'-' as description,
'-' as gl_cat,
acct.value as acct_value,
acct.name as acct_name,
Z_Id_Cpt_To_Val($P{Account_From}) as iFromAcc,
Z_Id_Cpt_To_Val($P{Account_To}) as iToAcc,
bse.iso_code as iso_code,
coalesce(z_account_starting_balance(acct.ad_client_id, $P{AD_Org_ID}, ci.c_acctschema1_id, acct.c_elementvalue_id, $P{DateFrom}),0) as starting_balance,
0 as ending_balance,
ac.name as client_name,
(select name from ad_org where ad_org_id = $P{AD_Org_ID}) as org_name,
bse.iso_code as base_curr,
0 as adTableID,
0 as RecordID,
'' as BPName,
'en_US' as AD_LANGUAGE
from c_elementvalue acct
inner join c_element e on e.c_element_id = acct.c_element_id and e.elementtype = 'A'
inner join ad_client ac on ac.ad_client_id = acct.ad_client_id
inner join ad_org org on org.ad_org_id = acct.ad_org_id
inner join ad_clientinfo ci on ac.ad_client_id = ci.ad_client_id
inner join c_acctschema cas on cas.c_acctschema_id = ci.c_acctschema1_id
inner join c_currency bse on bse.c_currency_id = cas.c_currency_id
where acct.issummary = 'N'
	and ( acct.ad_client_id = $P{AD_Client_ID} or  $P{AD_Client_ID} = 0 )
	and ( Z_Id_Cpt_To_Val(acct.c_elementvalue_id) >= Z_Id_Cpt_To_Val($P{Account_From}) or $P{Account_From} = 0)
	and ( Z_Id_Cpt_To_Val(acct.c_elementvalue_id) <= Z_Id_Cpt_To_Val($P{Account_To}) or $P{Account_To} = 0)
   	and ( exists (
					select * from fact_acct f where f.dateacct >= $P{DateFrom}
					and f.dateacct <= $P{DateTo}
					and acct.c_elementvalue_id = f.account_id and f.postingtype = 'A'
					and (f.ad_client_id = $P{AD_Client_ID} or  $P{AD_Client_ID} = 0)
 					and (f.C_Activity_ID = $P{C_Activity_ID} or $P{C_Activity_ID} is null or $P{C_Activity_ID} = 0)
					and (f.ad_Org_id = $P{AD_Org_ID} or  $P{AD_Org_ID} = 0)))

union
select coalesce(coalesce(f.amtacctdr,0) - coalesce(f.amtacctcr,0),0) as amount,
coalesce(f.amtacctcr, 0) as amountCR,
coalesce(f.amtacctdr, 0) as amountDR,
coalesce(coalesce(f.amtsourcedr,0) - coalesce(f.amtsourcecr,0),0) as source_amount,
coalesce(f.dateacct,current_date) as dateacct,
--coalesce(f.description || ' [' || f.fact_acct_id || ']',N'') as description,
coalesce(getdescriptionfact(f.fact_acct_id),'') as description,
coalesce(glc.name,N'') as gl_cat,
acct.value as acct_value,
acct.name as acct_name,
Z_Id_Cpt_To_Val($P{Account_From}) as iFromAcc,
Z_Id_Cpt_To_Val($P{Account_To}) as iToAcc,
c.iso_code as iso_code,
0 as starting_balance,
0 as ending_balance,
ac.name as client_name,
org.name as org_name,
bse.iso_code as base_curr,
f.AD_TABLE_ID as adTableID,
f.RECORD_ID as RecordID,
bp.name as BPName,
'en_US' as AD_LANGUAGE
from c_elementvalue acct
inner join c_element e on e.c_element_id = acct.c_element_id and e.elementtype = 'A'
left outer join fact_acct f on f.dateacct >= $P{DateFrom}
		and f.dateacct <= $P{DateTo}
		and acct.c_elementvalue_id = f.account_id and f.postingtype = 'A'
		and f.ad_client_id = $P{AD_Client_ID}
left outer join c_currency c on c.c_currency_id = f.c_currency_id
left outer join gl_category glc on glc.gl_category_id = f.gl_category_id
inner join ad_client ac on ac.ad_client_id = $P{AD_Client_ID}
inner join ad_org org on org.ad_org_id = $P{AD_Org_ID}
inner join ad_clientinfo ci on ci.ad_client_id = $P{AD_Client_ID}
inner join c_acctschema cas on cas.c_acctschema_id = ci.c_acctschema1_id
inner join c_currency bse on bse.c_currency_id = cas.c_currency_id
left outer join C_BPARTNER bp on (bp.C_BPARTNER_ID = f.C_BPARTNER_ID)
where acct.issummary = 'N'
	  and ( f.ad_client_id = $P{AD_Client_ID} or  $P{AD_Client_ID} = 0 )
      	  and ( acct.ad_client_id = 0 or acct.ad_client_id = $P{AD_Client_ID} or  $P{AD_Client_ID} = 0 )
	  and ( f.ad_org_id = $P{AD_Org_ID} or  $P{AD_Org_ID} = 0 )
	  and (f.C_Activity_ID = $P{C_Activity_ID} or $P{C_Activity_ID} is null or $P{C_Activity_ID} = 0)
	  and ( Z_Id_Cpt_To_Val(acct.c_elementvalue_id) >= Z_Id_Cpt_To_Val($P{Account_From}) or $P{Account_From} = 0)
	  and ( Z_Id_Cpt_To_Val(acct.c_elementvalue_id) <= Z_Id_Cpt_To_Val($P{Account_To}) or $P{Account_To} = 0)
          and ( exists (
					select * from fact_acct f where f.dateacct >= $P{DateFrom}
					and f.dateacct <= $P{DateTo}
					and acct.c_elementvalue_id = f.account_id and f.postingtype = 'A'
					and (f.ad_client_id = $P{AD_Client_ID} or  $P{AD_Client_ID} = 0)
					and (f.ad_org_id = $P{AD_Org_ID} or  $P{AD_Org_ID} = 0)))


	order by acct_value, dateacct, iso_code, adTableID, RecordID]]>
	</queryString>
