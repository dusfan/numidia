	<property name="com.jaspersoft.studio.data.sql.tables" value=""/>
	<property name="com.jaspersoft.studio.data.defaultdataadapter" value="Numidia-Test"/>
	<parameter name="RECORD_ID" class="java.lang.Integer"/>
